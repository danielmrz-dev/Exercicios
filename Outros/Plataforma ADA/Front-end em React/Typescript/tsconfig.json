    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node10",
        "outDir": "./dist",
        "noEmitOnError": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
